{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "CodeLens (Launch) - Test",
      "request": "launch",
      "mainClass": "io.inprice.manager.Test",
      "projectName": "manager"
    },
    {
      "type": "java",
      "name": "Debug API",
      "request": "launch",
      "projectName": "manager",
      "mainClass": "io.inprice.manager.Application",
      "env": {
        "APP_ENV": "DEV",
        "WAITING_TIME_FOR_FETCHING_COMPETITORS": 3,

        "TIMING_FOR_TOBE_CLASSIFIED_COMPETITORS": "10s",
        "TIMING_FOR_SOCKET_ERRORS": "10s",
        "TIMING_FOR_NO_DATA_ERRORS": "10d",

        "TIMING_FOR_AVAILABLE_COMPETITORS": "10d",
        "TIMING_FOR_NOT_AVAILABLE_COMPETITORS": "5d",
        "TIMING_FOR_RESUMED_COMPETITORS": "5d",
        "TIMING_FOR_TOBE_RENEWED_COMPETITORS": "7d",
        "TIMING_FOR_IMPLEMENTED_COMPETITORS": "5d",
        "TIMING_FOR_UPDATING_PRODUCT_PRICES": "15d",
        "TIMING_FOR_CLEANING_COMPETITORS": "10d",
        "TIMING_FOR_NETWORK_ERRORS": "5d",
        "TIMING_FOR_BLOCKED_COMPETITORS": "5d",
        "RETRY_LIMIT_FOR_FAILED_COMPETITORS_G1": 3,
        "RETRY_LIMIT_FOR_FAILED_COMPETITORS_G2": 5,
        "RETRY_LIMIT_FOR_FAILED_COMPETITORS_G3": 10
      }
    }
  ]
}

{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "CodeLens (Launch) - Test",
      "request": "launch",
      "mainClass": "io.inprice.manager.Test",
      "projectName": "manager"
    },
    {
      "type": "java",
      "name": "Debug API",
      "request": "launch",
      "projectName": "manager",
      "mainClass": "io.inprice.manager.Application",
      "env": {
        "APP_ENV": "DEV",
        "WAITING_TIME_FOR_FETCHING_LINKS": 3,

        "TIMING_FOR_TOBE_CLASSIFIED_LINKS": "10s",
        "TIMING_FOR_SOCKET_ERRORS": "10s",
        "TIMING_FOR_NO_DATA_ERRORS": "10d",

        "TIMING_FOR_AVAILABLE_LINKS": "10d",
        "TIMING_FOR_NOT_AVAILABLE_LINKS": "5d",
        "TIMING_FOR_RESUMED_LINKS": "5d",
        "TIMING_FOR_TOBE_RENEWED_LINKS": "7d",
        "TIMING_FOR_IMPLEMENTED_LINKS": "5d",
        "TIMING_FOR_UPDATING_PRODUCT_PRICES": "15d",
        "TIMING_FOR_CLEANING_MEMBERS": "10d",
        "TIMING_FOR_NETWORK_ERRORS": "5d",
        "TIMING_FOR_BLOCKED_LINKS": "5d",
        "RETRY_LIMIT_FOR_FAILED_LINKS_G1": 3,
        "RETRY_LIMIT_FOR_FAILED_LINKS_G2": 5,
        "RETRY_LIMIT_FOR_FAILED_LINKS_G3": 10
      }
    }
  ]
}
